<Window x:Class="GPM.Product.Mvpvm.View.AboutView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GPM.Product.Mvpvm.View"
        xmlns:i18n="clr-namespace:GPM.Product.Mvpvm.View.Localization"
        mc:Ignorable="d"
        Title="{x:Static i18n:AboutViewLocale.Title}" Height="117" Width="334">
    <Grid>
        <Label Content="{x:Static i18n:AboutViewLocale.Message}" HorizontalAlignment="Center" Margin="0,40,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
